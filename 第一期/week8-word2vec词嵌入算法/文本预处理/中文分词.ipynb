   "source": [
    "import jieba\n",
    "\n",
    "text = \"我来到北京清华大学\"\n",
    "jieba.add_word(\"北京清华大学\")\n",
    "jieba.lcut(text)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "我: r\n",
      "来到: v\n",
      "北京清华大学: x\n"
     ]
    }
   ],
   "source": [
    "import jieba.posseg as pseg\n",
    "\n",
    "words = pseg.lcut(text)\n",
    "for word, flag in words:\n",
    "    print(f'{word}: {flag}')\n",
    "# 输出：\n",
    "# 我: r\n",
    "# 来到: v\n",
    "# 北京: ns\n",
    "# 清华大学: nt"
   ]
  }
